<!DOCTYPE html>
<html>
<head>
  <title>EBS demo</title>
  <%= stylesheet_link_tag 'application' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tag %>
  <%= yield :head %>
</head>
<body>
  <div id="header">
    <div class="user_info">
      <% if current_user %>
        Logged in as
        <%= current_user.user_name %>
        |
        <%= link_to "Log out", logout_path %>
      <% end %>
    </div>
  </div>
  <div id="container">
    <% flash.each do |name, msg| %>
      <%= content_tag :div, msg, :id => "flash_#{name}" %>
    <% end %>
    <%= yield %>
  </div>
</body>
</html>
